{
  "name": "@synapsecns/sdk-router",
  "description": "An SDK for interacting with the Synapse Protocol",
  "version": "0.2.22",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synapsecns/sanguine.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "lint": "npm run lint:fix && npm run lint:check",
    "lint:check": "eslint . --max-warnings=0 --ext ts --ext tsx --ext js --ext jsx",
    "ci:lint": "npm run lint:check",
    "lint:fix": "eslint . --fix --max-warnings=0 --ext ts --ext tsx --ext js --ext jsx",
    "test:coverage": "vitest run --coverage",
    "prepare": "tsup",
    "preinstall": "command -v tsup >/dev/null 2>&1 && tsup || echo 'tsup not found'",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build:go": " ",
    "build:slither": " "
  },
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/sdk.ts",
    "readmeFile": "./README.md",
    "displayName": "SDK ROUTER",
    "tsconfig": "./tsconfig.json"
  },
  "module": "dist/sdk-v2.esm.js",
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "tsup": "^7.2.0",
    "typescript": "^4.8.3",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "ethers": "^5.7.2",
    "jsbi": "^4.3.0",
    "tiny-invariant": "^1.2.0",
    "ts-xor": "^1.1.0"
  }
}
